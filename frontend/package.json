{
  "name": "minimal-react-webpack-babel-setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "sideEffects": [
    "./src/initialize.js"
  ],
  "scripts": {
    "start": "webpack-dev-server --env.ENV=DEV --hot --progress",
    "start-prod": "webpack-dev-server --env.ENV=PRODUCTION",
    "build": "webpack --env.ENV=PRODUCTION",
    "test": "jest",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit",
    "lint": "./node_modules/.bin/eslint src",
    "lint-fixup": "./node_modules/.bin/eslint src --ext .js,.jsx --fix"
  },
  "keywords": [],
  "author": "Sergii Butenko",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "_setupFiles": [
      "<rootDir>/polyfills.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub"
    },
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-react": "^7.11.1",
    "fetch-mock": "^7.2.5",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "jest-junit": "^5.2.0",
    "jest-transform-stub": "^1.0.0",
    "node-sass": "^4.10.0",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "^4.3.11",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.0",
    "@material-ui/lab": "^3.0.0-alpha.19",
    "classnames": "^2.2.6",
    "eslint": "^5.6.1",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0-0",
    "redux-actions": "^2.6.1",
    "redux-auth-wrapper": "^2.0.3",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.2.0",
    "typeface-roboto": "^0.0.54"
  }
}
